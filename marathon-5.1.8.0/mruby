PROG=`which "$0"`
DIST=`dirname $PROG`

if [ "$DIST" = "." ]
then
  DIST=`pwd`
else
  SAVED="`pwd`"
  cd "$DIST"
  DIST="`pwd`"
  cd "$SAVED"
fi

MARATHONHOME=$DIST
export GEM_PATH=$MARATHONHOME/support/ruby-selenium-webdriver-5.1.8.0.jar

java -Dfile.encoding=utf8 -jar "$MARATHONHOME/support/jruby-complete-9.1.13.0.jar" $*
